	<record model="ir.ui.view" id="sb_sla_category_view_form">
        <field name="name">sb.sla.category.form</field>
        <field name="model">sb.sla.category</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<field name="name"/>
					</group>
				</sheet>
			</form>
        </field>
	</record>
	<record model="ir.ui.view" id="sb_sla_category_view_tree">
        <field name="name">sb.sla.category.tree</field>
        <field name="model">sb.sla.category</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
			<tree>
				<field name="name"/>
			</tree>
        </field>
	</record>
	<record id="action_sb_sla_category" model="ir.actions.act_window">
        <field name="name">SLA Category</field>
        <field name="res_model">sb.sla.category</field>
        <field name="view_mode">tree,form</field>
    </record>
	
	
	<record model="ir.ui.view" id="sb_sla_view_form">
        <field name="name">sb.sla.form</field>
        <field name="model">sb.sla</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<field name="name"/>
						<field name="sla_category_id"/>
					</group>
				</sheet>
			</form>
        </field>
	</record>
	<record model="ir.ui.view" id="sb_sla_view_tree">
        <field name="name">sb.sla.tree</field>
        <field name="model">sb.sla</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
			<tree>
				<field name="name"/>
				<field name="sla_category_id"/>
			</tree>
        </field>
	</record>
	<record id="action_sb_sla" model="ir.actions.act_window">
        <field name="name">SLA</field>
        <field name="res_model">sb.sla</field>
        <field name="view_mode">tree,form</field>
    </record>

